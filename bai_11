public class bai_11 {
	public void songuyento(float n)
	{
		int m = 1;
		if(n!=(int)n) m = 0;
		if(n<=1) m = 0;
		for(int i=2;i<n;i++)
			if(n%i==0) m = 0;
		if(m==0) System.out.printf("%.0f khong phai la so nguyen to",n);
		else System.out.printf("%.0f la so nguyen to",n);
	}
	public static void main(String[] args) {
		bai_11 bai11 = new bai_11();
		bai11.songuyento(7);
	}

}
