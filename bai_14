public class bai_14 {
	public void daysonguyento(int n)
	{
		System.out.println("in ra cac so nguyen to nho hon hoac bang n");
		for(int i=2;i<=n;i++)
		{
			int m=1;
			for(int j=2;j<i;j++)
				if(i%j==0) m = 0;
			if(m==1) System.out.printf("%-5d",i);
		}
	}
	public static void main(String[] args) {
		bai_14 bai14 = new bai_14();
		bai14.daysonguyento(20);
	}

}
