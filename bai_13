public class bai_13 {
	public void doixung(int n)
	{
		int m = n,s=0;
		while(m!=0)
		{
			s = s*10 + m%10;
			m /= 10;
		}
		if(s==n) System.out.printf("%d la so doi xung",n);
		else System.out.printf("%d khong phai la so doi xung",n);
	}
	public static void main(String[] args) {
		bai_13 bai13 = new bai_13();
		bai13.doixung(1221);
	}

}
