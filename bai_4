public class bai_4 {
	public void karaoke(int a,int b)
	{
		int c;
		if(b-a<=18) c =(b-a)*45000;
		else c = (b-a)*60000;
		System.out.println("so tien phai tra la: "+c);
	}
	public static void main(String[] args) {
		bai_4 bai4 = new bai_4();
		System.out.println("Tinh tien cho bai toan karaoke");
		bai4.karaoke(7, 21);
	}

}
